<%= form_for @backpack do |f| %>
  <p>
    <%= f.label :color %><br>
    <%= f.text_field :color %>
  </p>

  <p>
    <%= f.label :capacity %><br>
    <%= f.text_field :capacity %>
  </p>

  <p>
    <%= f.label :pockets %><br>
    <%= f.text_field :pockets %>
  </p>

  <p>
    <%= f.label :waterproof %><br>
    <%= f.text_field :waterp %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', backpacks_path %>
